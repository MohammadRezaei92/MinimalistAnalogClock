<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Clock">
        <attr name="hourDotColor" format="reference|color" />
        <attr name="minuteDotColor" format="reference|color" />
        <attr name="handsColor" format="reference|color" />
        <attr name="secondDotColor" format="reference|color" />
        <attr name="showDate" format="boolean" />
        <attr name="showSecond" format="boolean" />
        <attr name="datePrimaryColor" format="reference|color" />
        <attr name="dateSecondaryColor" format="reference|color" />
        <attr name="centerCirclePrimaryColor" format="reference|color" />
        <attr name="centerCircleSecondaryColor" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="ColorSeekBar">
        <attr name="colorSeeds" format="reference" />
        <attr name="cornerRadius" format="dimension"/>
        <attr name="barHeight" format="dimension"/>
        <attr name="thumbBorder" format="dimension"/>
        <attr name="thumbBorderColor" format="color"/>
    </declare-styleable>
</resources>
